{
  "name": "algedonic",
  "description": "algedonic",
  "version": "1.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "build:dev": "npm run build && npm run start",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "npx cypress run --reporter mochawesome",
    "cy:open": "npx cypress open",
    "test": "run-p --race dev cy:run",
    "instrument": "npx nyc instrument --compact=false src instrumented",
    "report-cover": "npx nyc report --reporter=text-summary",
    "lint": "eslint --ignore-path .eslintignore .",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte"
  },
  "dependencies": {
    "@babel/compat-data": "^7.8.0",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "compression": "^1.7.1",
    "csv": "^5.3.2",
    "devalue": "^2.0.1",
    "dotenv": "^8.2.0",
    "jquery": "^3.5.1",
    "mochawesome": "^6.1.0",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@beyonk/svelte-notifications": "^3.1.0",
    "@beyonk/svelte-scrollspy": "^2.1.2",
    "@cypress/code-coverage": "^3.5.3",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@smui/dialog": "^1.0.0",
    "@tivac/eslint-plugin-svelte": "^1.3.0",
    "aws-sdk": "^2.825.0",
    "cypress": "^4.11.0",
    "eslint": "^6.8.0",
    "express-session": "^1.17.0",
    "filepond": "^4.25.1",
    "filepond-plugin-image-exif-orientation": "^1.0.9",
    "filepond-plugin-image-preview": "^4.6.4",
    "fs": "0.0.1-security",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.1.2",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "prettier": "^1.16.4",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.16",
    "session-file-store": "^1.4.0",
    "simple-svelte-autocomplete": "^1.1.2",
    "sv-bootstrap-dropdown": "^0.4.3",
    "sv-bootstrap-modal": "^1.0.2",
    "svelte": "^3.18.1",
    "svelte-content-loader": "^1.1.3",
    "svelte-grid": "^1.11.2",
    "svelte-simple-modal": "^0.6.0",
    "sveltestrap": "^3.3.3"
  },
  "resolutions": { "@babel/preset-env": "7.5.5" },
  "lint-staged": {
    "*.{js,css,html}": "eslint --cache --fix",
    "*.{js,css,md,svelte}": "prettier --write"
  }
}
